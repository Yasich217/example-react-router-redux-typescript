{
  "compilerOptions": {
    "allowJs": true,
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "es2015",
      "es2016"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "target": "esnext",
    "removeComments": false,
    "preserveConstEnums": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "noEmitHelpers": false,
    "noEmitOnError": false,
    "noImplicitAny": false,
    "noErrorTruncation": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "baseUrl": ".",
    "jsx": "react"
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "exclude": [
    "node_modules",
    "out"
  ]
}